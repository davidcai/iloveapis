{
  "extends": "eslint:recommended",
  "plugins": ["angular"],
  "env": {
    "node": true,
    "browser": true,
    "jasmine": true
  },
  "rules": {
    "angular/controller-as": 0,
    "angular/controller-as-route": 0,
    "angular/controller-as-vm": 0,
    "angular/log": 0,
    "no-console": 0
  },
  "globals": {
    "angular": true,
    "module": true,
    "inject": true,
    "fe": true
  }
}
